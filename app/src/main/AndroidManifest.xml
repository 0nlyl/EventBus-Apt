<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shizhefei.eventbus.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name="com.shizhefei.eventbus.AppContext"
        android:theme="@style/AppTheme">
        <activity android:name="com.shizhefei.eventbus.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.shizhefei.eventbus.SecondActivity" android:process=":aaa"/>
        <service android:name="com.shizhefei.eventbus.EventBusService"/>
    </application>

</manifest>